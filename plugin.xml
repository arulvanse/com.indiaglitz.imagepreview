<?xml version="1.0" encoding="utf-8"?>
<plugin xmlns="http://www.phonegap.com/ns/plugins/1.0"
        id="com.indiaglitz.ImagePreview"
        version="1.3.5">

  <name>ImagePreview</name>
	<description>Testing Plugin for Development</description>
	<author>Arul</author>
	<license>MIT</license>
	<keywords>Testing</keywords>
  <engines>
    <engine name="cordova" version=">=3.4.0"/>
  </engines>

  <asset src="www/imagepreview.js" target="js/imagepreview.js"/>

  <js-module src="www/imagepreview.js" name="imagepreview">
    <clobbers target="imagepreview" />
  </js-module>

  <platform name="ios">
    
    <config-file target="config.xml" parent="/widget">
      <feature name="ImagePreview">
        <param name="ios-package" value="ImagePreview" />
      </feature>
    </config-file>

    <header-file src="src/ios/new/ImagePreview.h" target-dir="ImagePreviewPlugin"/>
    <source-file src="src/ios/new/ImagePreview.m" target-dir="ImagePreviewPlugin"/>
    
    <header-file src="src/ios/new/AsyncImageView.h" target-dir="ImagePreviewPlugin"/>
    <source-file src="src/ios/new/AsyncImageView.m" target-dir="ImagePreviewPlugin"/>
    
    <header-file src="src/ios/new/GallaryVC.h" target-dir="ImagePreviewPlugin"/>
    <source-file src="src/ios/new/GallaryVC.m" target-dir="ImagePreviewPlugin"/>
    <source-file src="src/ios/new/GallaryVC.xib" target-dir="ImagePreviewPlugin"/>
    
    <header-file src="src/ios/new/GallaryVC2.h" target-dir="ImagePreviewPlugin"/>
    <source-file src="src/ios/new/GallaryVC2.m" target-dir="ImagePreviewPlugin"/>
    <source-file src="src/ios/new/GallaryVC2.xib" target-dir="ImagePreviewPlugin"/>
    
    <header-file src="src/ios/new/zoomView.h" target-dir="ImagePreviewPlugin"/>
    <source-file src="src/ios/new/zoomView.m" target-dir="ImagePreviewPlugin"/>
    <source-file src="src/ios/new/zoomView.xib" target-dir="ImagePreviewPlugin"/>
    
    <header-file src="src/ios/new/ZoomingView.h" target-dir="ImagePreviewPlugin"/>
    <source-file src="src/ios/new/ZoomingView.m" target-dir="ImagePreviewPlugin"/>
    <source-file src="src/ios/new/ZoomingView.xib" target-dir="ImagePreviewPlugin"/>
    
    
    

    <source-file src="src/ios/1.jpg" target-dir="ImagePreviewPlugin"/>
    <source-file src="src/ios/2.jpg" target-dir="ImagePreviewPlugin"/>
    <source-file src="src/ios/3.jpg" target-dir="ImagePreviewPlugin"/>
    <source-file src="src/ios/4.jpg" target-dir="ImagePreviewPlugin"/>
    <source-file src="src/ios/5.jpg" target-dir="ImagePreviewPlugin"/>
    <source-file src="src/ios/6.jpg" target-dir="ImagePreviewPlugin"/>
    <source-file src="src/ios/7.jpg" target-dir="ImagePreviewPlugin"/>
    <source-file src="src/ios/8.jpg" target-dir="ImagePreviewPlugin"/>
    <source-file src="src/ios/9.jpg" target-dir="ImagePreviewPlugin"/>
    
    <source-file src="src/ios/new/close.png" target-dir="ImagePreviewPlugin"/>
    <source-file src="src/ios/new/close@2x.png" target-dir="ImagePreviewPlugin"/>
    <source-file src="src/ios/new/download.png" target-dir="ImagePreviewPlugin"/>
    <source-file src="src/ios/new/download@2x.png" target-dir="ImagePreviewPlugin"/>
    
    

    

    
  </platform>


</plugin>